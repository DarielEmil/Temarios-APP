{
   "version": 2,
   "builds": [
      {
         "src": "./index.js",
         "use": "@vercel/node"
      }
   ],
   "routes": [
      {
         "src": "/(.*)",
         "dest": "/",
         "headers": {
            "Access-Control-Allow-Credentials": true,
            "Access-Control-Allow-Origin": "https://tudominiofrontend.com",
            "Access-Control-Allow-Methods": "POST"
         }
      }
   ]
}
